; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://Bullet/Bullet.gd"
}, {
"base": "CanvasLayer",
"class": "DPanel",
"language": "GDScript",
"path": "res://Debug Panel/DebugPanel.gd"
}, {
"base": "Reference",
"class": "MyVector",
"language": "GDScript",
"path": "res://Lib/MyVector.gd"
}, {
"base": "Reference",
"class": "PolygonLib",
"language": "GDScript",
"path": "res://Lib/Polygon.gd"
} ]
_global_script_class_icons={
"Bullet": "",
"DPanel": "",
"MyVector": "",
"PolygonLib": ""
}

[application]

config/name="Goktasi"
run/main_scene="res://Main/Main.tscn"
config/icon="res://icon.png"

[autoload]

Gui="*res://GUI/GUI.tscn"

[display]

window/size/width=640
window/size/height=1024
window/size/fullscreen=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Ship"
2d_physics/layer_2="MeteorArea"
2d_physics/layer_3="BulletTravelLimitArea"
2d_physics/layer_4="Bullet"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.168627, 0.203922, 0.403922, 1 )
environment/default_environment="res://default_env.tres"
